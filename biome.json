{
	"files": {
		"ignore": [
			".nyc_output",
			"coverage",
			"schemas",
			"schemas.orig",
			"dummy.json",
			"package.json"
		]
	},
	"formatter": {
		"lineEnding": "lf"
	},
	"json": {
		"formatter": {
			"lineEnding": "lf"
		}
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"complexity": { "useLiteralKeys": "off" }
		}
	}
}
